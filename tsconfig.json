{
  "compilerOptions": {
    "allowJs": false,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "declaration": true,
    "removeComments": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "importHelpers": true,
    "target": "es2016",
    "lib": ["es2016"],
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "moduleResolution": "node",
    "types": ["node"]
  },
  "include": [
  ],
  "files": [
    "src/index.ts",
    "src/webpack-plugin/muskad.ts"
  ]
}
